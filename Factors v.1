
  if(ibool > 1)
  {
    cnt++;
  }
  return cnt == 2;
}
 
void displayNum(int lo, int hi)
{
  vector<int> num;
  cout << "All the numbers between " << lo << " and " << hi << " whose factors are exactly two prime numbers: \n";
 
  for(int i = lo; i <= hi; i++)
  {
    if(primprod(i))
    {
      num.push_back(i);
    }
  }
 
  for(int i = 0; i < num.size(); i ++)
  {
    cout << num[i] << '\t';
  }
  exit(0);
}
 
